{
  "version": 2,
  "name": "kattsafe-pdf-qa",
  "builds": [
    {
      "src": "pdf-qa-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/pdf-qa-frontend/build/static/$1"
    },
    {
      "src": "/manifest.json",
      "dest": "/pdf-qa-frontend/build/manifest.json"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "dest": "/pdf-qa-frontend/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/pdf-qa-frontend/build/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "@api_url"
  },
  "build": {
    "env": {
      "REACT_APP_API_URL": "@api_url"
    }
  }
}
